  object updCliente: TZUpdateSQL
    DeleteSQL.Strings = (
      'DELETE FROM clientes'
      'WHERE'
      '  clientes.id = :OLD_id')
    InsertSQL.Strings = (
      'INSERT INTO clientes'
      
        '  (id, id_cidade, nome, tipo, sexo, cpf, rg, cep, endereco, num_' +
        'end, bairro, '
      
        '   complemento, insc_estadual, celular, telefone, email, data_na' +
        'scimento, '
      '   data_cadastro)'
      'VALUES'
      
        '  (:id, :id_cidade, :nome, :tipo, :sexo, :cpf, :rg, :cep, :ender' +
        'eco, :num_end, '
      
        '   :bairro, :complemento, :insc_estadual, :celular, :telefone, :' +
        'email, '
      '   :data_nascimento, :data_cadastro)')
    ModifySQL.Strings = (
      'UPDATE clientes SET'
      '  id = :id,'
      '  id_cidade = :id_cidade,'
      '  nome = :nome,'
      '  tipo = :tipo,'
      '  sexo = :sexo,'
      '  cpf = :cpf,'
      '  rg = :rg,'
      '  cep = :cep,'
      '  endereco = :endereco,'
      '  num_end = :num_end,'
      '  bairro = :bairro,'
      '  complemento = :complemento,'
      '  insc_estadual = :insc_estadual,'
      '  celular = :celular,'
      '  telefone = :telefone,'
      '  email = :email,'
      '  data_nascimento = :data_nascimento,'
      '  data_cadastro = :data_cadastro'
      'WHERE'
      '  clientes.id = :OLD_id')
    UseSequenceFieldForRefreshSQL = False
    Left = 912
    Top = 64
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'id'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'id_cidade'
        ParamType = ptUnknown
