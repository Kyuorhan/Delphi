  object sqlContas_Receber: TZQuery
    Connection = frmPrincipal.ZConnection1
    UpdateObject = updReceber
    Active = True
    SQL.Strings = (
      
        'SELECT r.* , COALESCE(c.nome, '#39#39') as nome, COALESCE(t.documento,' +
        ' '#39#39') as documento'
      'FROM contas_receber r'
      'LEFT JOIN clientes c ON c.id = r.id_cliente'
      'LEFT JOIN tipos_documentos t ON t.id = r.id_documento')
    Params = <>
    Left = 945
    Top = 8
    object sqlContas_Receberid: TIntegerField
      DisplayLabel = 'ID'
      FieldName = 'id'
      Required = True
    end
    object sqlContas_Receberid_cliente: TIntegerField
      DisplayLabel = 'ID/CLIENTE'
      FieldName = 'id_cliente'
    end
    object sqlContas_Receberid_documento: TIntegerField
      DisplayLabel = 'ID/DOCMENT.'
      FieldName = 'id_documento'
    end
    object sqlContas_Receberdescricao: TWideStringField
      DisplayLabel = 'DESCRI'#199#195'O'
      FieldName = 'descricao'
      Size = 75
    end
    object sqlContas_Receberdata_documento: TDateField
      DisplayLabel = 'DATA/DOCMENT.'
      FieldName = 'data_documento'
    end
    object sqlContas_Receberdata_vencimento: TDateField
      DisplayLabel = 'DATA/VENCMENT.'
      FieldName = 'data_vencimento'
    end
    object sqlContas_Recebervalor: TFloatField
      DisplayLabel = 'VALOR'
      FieldName = 'valor'
      Required = True
    end
    object sqlContas_Receberdesconto: TFloatField
      DisplayLabel = 'DESCONTO'
      FieldName = 'desconto'
      Required = True
    end
    object sqlContas_Receberpago: TWideStringField
      DisplayLabel = 'PAGO'
      FieldName = 'pago'
      Size = 1
    end
    object sqlContas_Receberdata_cadastro: TDateField
      DisplayLabel = 'DATA/PAGO'
      FieldName = 'data_cadastro'
    end
    object sqlContas_Receberobs_quitacao: TWideStringField
      DisplayLabel = 'OBS/QUITA'#199#195'O'
      FieldName = 'obs_quitacao'
    end
  end
  object updReceber: TZUpdateSQL
    DeleteSQL.Strings = (
      'DELETE FROM contas_receber'
      'WHERE'
      '  contas_receber.id = :OLD_id')
    InsertSQL.Strings = (
      'INSERT INTO contas_receber'
      
        '  (id, id_cliente, id_documento, descricao, data_documento, data' +
        '_vencimento, '
      '   valor, desconto, pago, data_cadastro, obs_quitacao)'
      'VALUES'
      
        '  (:id, :id_cliente, :id_documento, :descricao, :data_documento,' +
        ' :data_vencimento, '
      '   :valor, :desconto, :pago, :data_cadastro, :obs_quitacao)')
    ModifySQL.Strings = (
      'UPDATE contas_receber SET'
      '  id = :id,'
      '  id_cliente = :id_cliente,'
      '  id_documento = :id_documento,'
      '  descricao = :descricao,'
      '  data_documento = :data_documento,'
      '  data_vencimento = :data_vencimento,'
      '  valor = :valor,'
      '  desconto = :desconto,'
      '  pago = :pago,'
      '  data_cadastro = :data_cadastro,'
      '  obs_quitacao = :obs_quitacao'
      'WHERE'
      '  contas_receber.id = :OLD_id')
    UseSequenceFieldForRefreshSQL = False
    Left = 1032
    Top = 72
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'id'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'id_cliente'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'id_documento'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'descricao'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'data_documento'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'data_vencimento'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'valor'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'desconto'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'pago'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'data_cadastro'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'obs_quitacao'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'OLD_id'
        ParamType = ptUnknown
      end>
  end
